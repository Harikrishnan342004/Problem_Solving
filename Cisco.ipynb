    "#where ever you find \"A\" replace it with 0 \n",
    "def  replace_A(x):\n",
    "    result = \"\"\n",
    "    for i in x:\n",
    "        if(i == \"a\"):\n",
    "            result += \"0\"\n",
    "        else:\n",
    "            result += i\n",
    "    return result\n",
    "x = \"Harikavitha\"\n",
    "y = replace_A(x)\n",
    "print(y)"
