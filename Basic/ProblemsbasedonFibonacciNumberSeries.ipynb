    "# fibonacci series\n",
    "def fibonacci(n):\n",
    "    series = [ ]\n",
    "    total = 0\n",
    "    a , b = 0 , 1\n",
    "    for _ in range(n):\n",
    "        series.append(a)\n",
    "        a , b =  b , a+b\n",
    "    for i in series:\n",
    "        total += i\n",
    "    print(total)\n",
    "    return series\n",
    "n = 4\n",
    "print(fibonacci(n))"
   ]
