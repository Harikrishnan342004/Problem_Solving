    "def Palindrome(Input):\n",
    "    Lenght = len(Input)\n",
    "    for i in range(0 , Lenght//2 ):\n",
    "        if( Input[i] != Input[Lenght - i - 1]):\n",
    "            return \"NO\"\n",
    "    return \"Yes\"\n",
    "Input = input(\"Enter the Palindrome\")\n",
    "output = Palindrome(Input)\n",
    "print(output)"
   ]
  }
